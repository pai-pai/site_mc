<h3><%= t('shared.pages.statistic.title') %></h3>
<div class="set_group">
    <%#= label_tag 'period', t('shared.pages.statistic.table.period') %>
    <%#= select("period", "start_date", @start_dates.collect { |data| [data.start_date, data.start_date] if not data.start_date.blank? }) %>
</div>
<div style="float: right;">
    <ul class="inline">
        <li><a href="statistic.xls" title=<%= t('shared.pages.statistic.excel.link') %>><i class="icon-file"></i></a></li>
        <li><a href="print-records" title=<%= t('shared.pages.statistic.printrecords') %> target="blank"><i class="icon-print"></i></a></li>
    </ul>
</div>
<p><strong><%= t('shared.pages.statistic.count_forms') %></strong><%= @count %></p>
